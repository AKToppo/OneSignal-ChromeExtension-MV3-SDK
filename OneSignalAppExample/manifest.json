{
  "name": "OneSignal Example",
  "description": "OneSignal Push Notifications Example Chrome app.",
  "manifest_version": 3,
  "version": "1.0.1",
  "app": {
    "background": {
      "scripts": ["OneSignal.js", "background.js"]
    }
  },
  "permissions": ["gcm", "storage", "notifications", "identity", "browser"],
  "icons": { "128": "OneSignal_icon.png" }
}